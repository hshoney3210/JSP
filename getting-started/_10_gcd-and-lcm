//https://classroom.pepcoding.com/myClassroom/the-switch-program-4/getting-started/gcd-and-lcm-official/ojquestion
import java.util.*;
    
    public class Main{
    
    public static void main(String[] args) {
      // write your code here  
      Scanner sc = new Scanner(System.in);
      int n1 = sc.nextInt();
      int n2 = sc.nextInt();
      
      int dvd = n1;
      int div = n2;
      
      while(dvd % div != 0){
          int rem = dvd % div;
          dvd = div;
          div = rem;
      }
      System.out.println(div);
      System.out.println((n1 * n2) / div);
     }
    }
