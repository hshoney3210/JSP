//https://classroom.pepcoding.com/myClassroom/the-switch-program-4/functions-and-arrays/find-element-in-array-official/ojquestion
import java.io.*;
import java.util.*;

public class Main{

public static void main(String[] args) throws Exception {
    Scanner sc = new Scanner(System.in);
    int n = sc.nextInt();
    int[] arr = new int[n];
    for(int i = 0; i < n; i++){
        arr[i] = sc.nextInt();
    }
    boolean found = false;
    int find = sc.nextInt();
    for(int i = 0; i < n; i++){
        if(find == arr[i]){
            System.out.println(i);
            found = true;
            break;
        }    
    }
    if(!found) System.out.println("-1");
 }

}import java.io.*;
import java.util.*;

